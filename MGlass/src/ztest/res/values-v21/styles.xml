<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="BaseAppTheme" parent="@android:style/Theme.Material.Wallpaper">
        <item name="android:textColorPrimaryDisableOnly">#ffffffff</item>
        <item name="android:windowBackground">@android:drawable/screen_background_dark_transparent</item>
        <item name="android:windowShowWallpaper">true</item>
        <item name="android:colorBackgroundCacheHint">#95000000</item>
        <item name="android:actionMenuTextColor">#ffffffff</item>
        <item name="android:windowContentTransitions">true</item>
        <item name="android:colorPrimary">#ff040404</item>
        <item name="android:colorPrimaryDark">#de000000</item>
        <item name="android:colorAccent">#ff14e5f4</item>
        <item name="android:statusBarColor">#de000000</item>
        <item name="android:navigationBarColor">#00000000</item>
    </style>
    <style name="BaseActionBarOverflow" parent="@android:style/Widget.Material.ActionButton.Overflow" />
    <style name="BaseAutoCompleteTextView" parent="@android:style/Widget.Material.AutoCompleteTextView" />
    <style name="BaseDialogThemeWhenLarge" parent="@android:style/Theme.Material.DialogWhenLarge" />
    <style name="BaseDialogTheme" parent="@android:style/Theme.Material.Dialog.MinWidth" />
    <style name="CakemixTheme.Upload" parent="@android:style/Theme.Material.Wallpaper" />
    <style name="ProgressBarSpinner" parent="@android:style/Widget.Material.ProgressBar.Large">
        <item name="android:indeterminateOnly">true</item>
    </style>
    <style name="Platform.AppCompat.Light" parent="@android:style/Theme.Material.Wallpaper">
        <item name="android:textColorPrimaryDisableOnly">#ffffffff</item>
        <item name="android:windowBackground">@android:drawable/screen_background_dark_transparent</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowShowWallpaper">true</item>
        <item name="android:colorBackgroundCacheHint">#95000000</item>
        <item name="android:windowActionBar">false</item>
        <item name="android:buttonBarStyle">?buttonBarStyle</item>
        <item name="android:buttonBarButtonStyle">?buttonBarButtonStyle</item>
        <item name="android:actionMenuTextColor">#ffffffff</item>
        <item name="android:navigationBarColor">#95000000</item>
    </style>
    <style name="Platform.AppCompat" parent="@android:style/Theme.Material.Wallpaper">
        <item name="android:windowBackground">@android:drawable/screen_background_dark_transparent</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowShowWallpaper">true</item>
        <item name="android:colorBackgroundCacheHint">#95000000</item>
        <item name="android:windowActionBar">false</item>
        <item name="android:buttonBarStyle">?buttonBarStyle</item>
        <item name="android:buttonBarButtonStyle">?buttonBarButtonStyle</item>
        <item name="android:navigationBarColor">#00000000</item>
    </style>
    <style name="Platform.AppCompat.Light.Dialog" parent="@android:style/Theme.Material.Dialog">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowActionBar">false</item>
    </style>
    <style name="Base.TextAppearance.AppCompat.Widget.ActionBar.Title" parent="@android:style/TextAppearance.Material.Widget.ActionBar.Title" />
    <style name="Base.TextAppearance.AppCompat.Widget.ActionBar.Subtitle" parent="@android:style/TextAppearance.Material.Widget.ActionBar.Subtitle" />
    <style name="Base.Widget.AppCompat.ActionButton.Overflow" parent="@android:style/Widget.Material.ActionButton.Overflow" />
    <style name="Base.Widget.AppCompat.ActionButton" parent="@android:style/Widget.Material.ActionButton" />
    <style name="CakemixTheme.Projector" parent="@android:style/Theme.Material.Wallpaper">
        <item name="android:colorBackground">#cc000000</item>
        <item name="android:actionBarStyle">@style/ProjectorActionBar</item>
    </style>
    <style name="BaseDialogThemeNoFrame" parent="@android:style/Theme.Material.Dialog.MinWidth">
        <item name="android:colorBackground">#cc000000</item>
        <item name="android:windowBackground">@android:drawable/screen_background_dark_transparent</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowAnimationStyle">@null</item>
        <item name="android:backgroundDimEnabled">false</item>
        <item name="android:windowCloseOnTouchOutside">false</item>
        <item name="android:windowElevation">0.0dip</item>
    </style>
    <style name="Base.Widget.AppCompat.Light.PopupMenu" parent="@android:style/Widget.Material.PopupMenu" />
    <style name="Base.Widget.AppCompat.Light.ActionBar.TabText" parent="@android:style/Widget.Material.ActionBar.TabText" />
    <style name="Base.Widget.AppCompat.Light.ActionBar.TabText.Inverse" parent="@android:style/Widget.Material.ActionBar.TabText" />
    <style name="Base.Widget.AppCompat.Light.ActionBar.TabView" parent="@android:style/Widget.Material.ActionBar.TabView" />
    <style name="Base.ThemeOverlay.AppCompat.Light" parent="@android:style/ThemeOverlay.Material.Dark">
        <item name="colorControlHighlight">#ff14e5f4</item>
        <item name="isLightTheme">false</item>
    </style>
    <style name="ProgressBar.Horizontal" parent="@android:style/Widget.Material.ProgressBar.Horizontal">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:indeterminateOnly">false</item>
        <item name="android:indeterminateBehavior">repeat</item>
    </style>
    <style name="UserFeedback" parent="@android:style/Theme.Material.Wallpaper" />
    <style name="UserFeedback_Tablet" parent="@android:style/Theme.Material.Dialog" />
    <style name="Base.TextAppearance.AppCompat.Title" parent="@android:style/TextAppearance.Material.Title" />
    <style name="BaseActionBar" parent="@android:style/Widget.Material.ActionBar.Solid">
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:background">#b3000000</item>
        <item name="android:windowShowWallpaper">true</item>
        <item name="android:backgroundStacked">#b3000000</item>
        <item name="android:backgroundSplit">#b3000000</item>
        <item name="android:elevation">0.0dip</item>
    </style>
    <style name="Base.Widget.AppCompat.ProgressBar.Horizontal" parent="@android:style/Widget.Material.ProgressBar.Horizontal" />
</resources>
